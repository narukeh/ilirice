#!/bin/sh
# Usage: beep -f FREQ -l LEN -d DELAY -r COUNT -n
# 	-f	Frequency in Hz
# 	-l	Length in ms
# 	-d	Delay in ms
# 	-r	Repetitions
# 	-n	Start new tone

asdf() {
/tmp/beep -f 40 	-l 700
/tmp/beep -f 500	-l 100	-d 150	-r 7 -n -f 550	-l 200 -d 150 -r 1
/tmp/beep -f 400	-l 100	-d 150	-r 7 -n -f 450	-l 200 -d 150 -r 1
/tmp/beep -f 300	-l 100	-d 150	-r 7 -n -f 350	-l 200 -d 150 -r 1
/tmp/beep -f 200	-l 100	-d 150	-r 7 -n -f 250	-l 200 -d 150 -r 1
/tmp/beep -f 80 	-l 400
}



beepxzb64=''

sudo modprobe pcspkr
[ ! -e /tmp/beep ] && echo "$beepxzb64" | base64 -di > /tmp/beep.xz && unxz /tmp/beep.xz && chmod +x /tmp/beep
asdf
sudo rmmod pcspkr
# vim:set ts=4:
